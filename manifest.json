{
    "manifest_version": 3,
    "name": "distractify",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["https://*/*"],
            "js": ["./main.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "32": "distract.png"
    }
}